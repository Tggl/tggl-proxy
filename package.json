{
  "name": "tggl-proxy",
  "version": "2.1.3",
  "description": "Tggl Proxy to self-host",
  "keywords": [
    "Tggl",
    "Proxy",
    "Feature flag",
    "Feature toggle",
    "A/B testing",
    "Remote config",
    "Kill switch"
  ],
  "author": "Nicolas Keller",
  "license": "ISC",
  "homepage": "https://tggl.io/developers/evaluating-flags/tggl-proxy",
  "repository": "https://github.com/Tggl/tggl-proxy",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/.",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "npm test && rm -rf lib && npm run build",
    "prepare": "husky install",
    "version": "node scripts/syncVersion.mjs && npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publishDocker": "docker buildx build . --platform linux/amd64,linux/arm64 -t tggl/tggl-proxy:2.1.3 -t tggl/tggl-proxy:latest --push"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.10.9",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "2.3.0",
    "ts-jest": "^29.1.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "case": "^1.6.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.12",
    "tggl-client": "^2.0.1",
    "winston": "^3.13.0"
  },
  "files": [
    "lib/**/*"
  ]
}
